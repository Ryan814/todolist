<div class="itembox">
	<p class ="title"><%= @todo.title %></p><br>
	<p class="due_date"><%= @todo.due_date %></p><br>
	<%= simple_format @todo.note, class: 'note' %>
	<div class="itembox_btn">
    <%= todo_edit_button(@todo) %><br>
    <%= link_to 'BACK', todos_path, class: 'btn btn-default' %>
  </div>
</div>